server {
        listen 80;
        server_name 18.211.36.253;
        return 301 http://madhukarreddy.com$;
        location / {
                proxy_pass http://localhost;
                include proxy_params;
        }
}

server {
        listen 443 ssl;
        server_name 18.211.36.253;
	return 301 https://madhukarreddy.com;

        ssl_certificate /etc/letsencrypt/live/madhukarreddy.com-0001/fullchain.pem;
        ssl_certificate_key /etc/letsencrypt/live/madhukarreddy.com-0001/privkey.pem;


	location / {
                root /var/www/html;
		index index.html index.htm;
        }
}